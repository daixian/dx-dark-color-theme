  "tokenColors": [
    {
      "name": "Default",
      "scope": [
        "source",
        "text"
      ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#699856"
      }
    },
    {
      "name": "Doc Comment",
      "scope": [
        "comment.block.documentation",
        "comment.line.documentation"
      ],
      "settings": {
        "foreground": "#6A9955"
      }
    },
    {
      "name": "Doc Tags",
      "scope": [
        "storage.type.class.jsdoc",
        "entity.name.type.instance.jsdoc",
        "comment.block.documentation keyword"
      ],
      "settings": {
        "foreground": "#569CD5"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#499CD5"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#CD9069"
      }
    },
    {
      "name": "String Escape",
      "scope": [
        "constant.character.escape",
        "string.regexp punctuation.definition.character-class"
      ],
      "settings": {
        "foreground": "#D7BA7D"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language.number"
      ],
      "settings": {
        "foreground": "#B4CDA8"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant.language",
        "constant.character",
        "support.constant"
      ],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "meta.definition.variable name"
      ],
      "settings": {
        "foreground": "#94DBFD"
      }
    },
    {
      "name": "Parameter",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#F49292"
      }
    },
    {
      "name": "Special Variable",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.self"
      ],
      "settings": {
        "foreground": "#569CD5"
      }
    },
    {
      "name": "Global Variable",
      "scope": [
        "variable.other.global",
        "variable.other.readwrite.global"
      ],
      "settings": {
        "foreground": "#AFDC50"
      }
    },
    {
      "name": "Static Member",
      "scope": [
        "variable.other.constant",
        "meta.object-literal.key",
        "entity.name.constant"
      ],
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "Function Declaration",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "Function Call",
      "scope": [
        "meta.function-call",
        "variable.function",
        "entity.name.function.member"
      ],
      "settings": {
        "foreground": "#DBDBAA"
      }
    },
    {
      "name": "Class And Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#4EC8B0"
      }
    },
    {
      "name": "Interface",
      "scope": [
        "entity.name.interface"
      ],
      "settings": {
        "foreground": "#3D8F7F"
      }
    },
    {
      "name": "Type Parameter",
      "scope": [
        "entity.name.type.parameter",
        "entity.name.type.type-parameter"
      ],
      "settings": {
        "foreground": "#39C8B0"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "meta.annotation",
        "entity.name.function.decorator",
        "entity.name.tag.decorator"
      ],
      "settings": {
        "foreground": "#39C8B0"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name",
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "C# Property (TextMate Fallback)",
      "scope": [
        "variable.other.property.cs",
        "variable.other.readwrite.property.cs",
        "entity.name.variable.property.cs"
      ],
      "settings": {
        "foreground": "#B3A3D7"
      }
    },
    {
      "name": "C# Static Property (TextMate Fallback)",
      "scope": [
        "variable.other.property.static.cs",
        "variable.other.readwrite.static.property.cs",
        "entity.name.variable.static.property.cs"
      ],
      "settings": {
        "foreground": "#B3A3D7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "C# Field (TextMate Fallback)",
      "scope": [
        "variable.other.field.cs",
        "variable.other.readwrite.field.cs",
        "entity.name.variable.field.cs"
      ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "C# Static Field (TextMate Fallback)",
      "scope": [
        "variable.other.field.static.cs",
        "variable.other.readwrite.static.cs"
      ],
      "settings": {
        "foreground": "#D4D4D4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "C# Const Field (TextMate Fallback)",
      "scope": [
        "variable.other.constant.cs",
        "variable.other.constant.static.cs",
        "entity.name.variable.constant.cs"
      ],
      "settings": {
        "foreground": "#FFC66D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Regex",
      "scope": [
        "string.regexp",
        "constant.regexp"
      ],
      "settings": {
        "foreground": "#D16969",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#499CD5"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#699856"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.raw.block",
        "fenced_code.block.language"
      ],
      "settings": {
        "foreground": "#CD905B"
      }
    },
    {
      "name": "Link",
      "scope": [
        "string.other.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#569CD6",
        "fontStyle": "underline"
      }
    }
    ,
    {
      "name": "Doc Comment Tags & Delimiters",
      "scope": [
        "entity.name.tag.localname.cs",
        "entity.name.tag.cs",
        "meta.tag.cs",
        "comment.block.documentation punctuation.definition.tag",
        "punctuation.definition.tag",
        "comment.documentation.delimiter",
        "comment.documentation.delimiter.cs",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#4C5C4C"
      }
    }
    ,
    {
      "name": "Doc Comment Attributes & Quotes",
      "scope": [
        "entity.other.attribute-name.localname.cs",
        "entity.other.attribute-name.cs",
        "comment.block.documentation.cs entity.other.attribute-name",
        "comment.block.documentation entity.other.attribute-name",
        "punctuation.definition.string.begin.cs",
        "punctuation.definition.string.end.cs",
        "string.quoted.double.cs",
        "comment.block.documentation string.quoted.double"
      ],
      "settings": {
        "foreground": "#4C5C4C"
      }
    }
  ],
